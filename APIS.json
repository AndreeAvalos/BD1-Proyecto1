    "Id_maestro": 1,
    "Id_calificacion": 0,
    "Id_examen": 0,
    "Id_actividad": 0,
    "Id_material": 0,
    "Id_tipo": 5,//TIPO 1 = ACTIVIDAD, TIPO 2 = EXAMEN, TIPO 3 = CALIFICACION, TIPO 4 = MATERIAL DE APOYO, TIPO 5 = AVISO
